    "gitAllowedExpressions": [],
    "gitDisallowedExpressions": [],
    "emailSubscribers": [],
    "name": "BTP CF import source",
    "tenantId": "teched2025-00",
    "state": "ENABLED",
    "systemManagementType": "CUSTOMER_MANAGED",
    "properties": {
        "Authentication": "BasicAuthentication",
        "ips.application.id": "",
        "ips.trace.failed.entity.content": "false",
        "ips.trace.skipped.entity.content": "false",
        "OAuth2TokenServiceURL": "",
        "ProxyType": "Internet",
        "Type": "HTTP",
        "URL": "https://api.authentication.eu10.hana.ondemand.com",
        "User": "",
        "xsuaa.group.prefix": "TECHED_BTP_"
    },
    "encryptedProperties": {
        "Password": null
    },
    "automaticOutboundCertificateRenew": false,
    "transformationChanged": true,
    "readTransformationChanged": false,
    "writeTransformationChanged": false,
    "fromImport": false,
    "transformation": {
        "group": {
            "condition": "$.displayName =~ /FINANCE_ADMIN_.*/",
            "mappings": [
                {
                    "sourcePath": "$.id",
                    "targetVariable": "entityIdSourceSystem"
                },
                {
                    "sourcePath": "$.displayName",
                    "targetPath": "$.displayName",
                    "functions": [
                        {
                            "function": "concatString",
                            "condition": "'%xsuaa.group.prefix%' !== 'null'",
                            "prefix": "%xsuaa.group.prefix%"
                        }
                    ]
                },
                {
                    "sourcePath": "$.description",
                    "optional": true,
                    "targetPath": "$.description"
                },
                {
                    "sourcePath": "$.zoneId",
                    "optional": true,
                    "targetPath": "$.zoneId"
                },
                {
                    "sourcePath": "$.members",
                    "preserveArrayWithSingleElement": true,
                    "optional": true,
                    "targetPath": "$.members"
                },
                {
                    "condition": "'%ips.application.id%' !== 'null'",
                    "constant": "%ips.application.id%",
                    "targetPath": "$['urn:ietf:params:scim:schemas:extension:sap:2.0:Group']['applicationId']"
                },
                {
                    "constant": "authorization",
                    "targetPath": "$['urn:ietf:params:scim:schemas:extension:sap:2.0:Group']['type']"
                },
                {
                    "sourcePath": "$['urn:ietf:params:scim:schemas:extension:sap:2.0:Group']['supportedOperations']",
                    "optional": true,
                    "targetPath": "$['urn:ietf:params:scim:schemas:extension:sap:2.0:Group']['supportedOperations']"
                }
            ]
        }
    },
    "gitFetchAllowed": false
}